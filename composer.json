{
	"name": "utk/coefoodsafetytools",
	"description": "WordPress theme and plugin development",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org",
			"only": [
				"wpackagist-plugin/*",
				"wpackagist-theme/*"
			]
		}
	],
	"require": {
		"wpackagist-plugin/what-the-file": "^1.5",
		"wpackagist-plugin/debug-this": "^0.6.3",
		"wpackagist-plugin/custom-post-type-ui": "^1.8",
		"wpackagist-plugin/debug-bar": "^1.1",
		"wpackagist-plugin/debug-bar-actions-and-filters-addon": "^1.5",
		"wpackagist-plugin/query-monitor": "^3.6"
	},
	"extra": {
		"installer-paths": {
			"plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"themes/{$name}/": [
				"type:wordpress-theme"
			]
		}
	},
	"require-dev": {
		"automattic/vipwpcs": "^2.2",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"phpcompatibility/phpcompatibility-wp": "2.1.0"
	}
}
