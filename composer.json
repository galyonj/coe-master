{
	"name": "utk/coefoodsafetytools",
	"description": "WordPress theme and plugin development",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org",
			"only": [
				"wpackagist-plugin/*",
				"wpackagist-theme/*"
			]
		}
	],
	"require": {
		"wpackagist-plugin/what-the-file": "^1.5",
		"wpackagist-plugin/debug-this": "^0.6.3",
		"wpackagist-plugin/all-meta-tags": "^4.41",
		"wpackagist-plugin/custom-post-type-ui": "^1.8"
	},
	"extra": {
		"installer-paths": {
			"plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"themes/{$name}/": [
				"type:wordpress-theme"
			]
		}
	},
	"require-dev": {
	}
}
